#include <iostream>

using namespace std;

int main()
{
	
int a = 5, b = 5, c = 10, result;

result = (a = b) && (c > b);
cout<<"(a = b) && (c > b) equals to "<<result<<endl;

result = (a = b) && (c < b);
cout<<"(a = b) && (c < b) equals to "<<result<<endl;

result = (a = b) || (c < b);
cout<<"(a = b) || (c < b) equals to "<<result<<endl;

result = (a != b) || (c < b);
cout<<"(a != b) || (c < b) equals to "<<result<<endl;

result = !(a != b);
cout<<"!(a == b) equals to "<<result<<endl;

result = !(a == b);
cout<<"!(a == b) equals to "<<result<<endl;

return 0;
}
